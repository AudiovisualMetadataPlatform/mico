  </route>
  <route id='workflow-simple1-pipeline-0'>
    <from uri='direct:workflow-simple1-pipeline-0'/>
    <pipeline>
      <to uri='mico-comp:vbox1?host=mico-platform&amp;extractorId=urn:org.example.services&amp;extractorVersion=1.0.0&amp;modeId=ABService'/>
      <to uri='mico-comp:vbox1?host=mico-platform&amp;extractorId=urn:org.example.services&amp;extractorVersion=1.0.0&amp;modeId=BCService'/>
      <to uri="mock:result_simple1"/>
