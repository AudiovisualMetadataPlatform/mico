# Inputs:
#   $(body) : The uri of the body
#   $(type) : The refined type as string
#   $(value) : Extractor result as string
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX mico: <http://www.mico-project.eu/ns/platform/1.0/schema#>

INSERT DATA {
  GRAPH <$(body)> {
    <$(body)> rdf:type mico:KeyFrameBody .
    <$(body)> mico:hasConfidence "$(value)" .
  }
}