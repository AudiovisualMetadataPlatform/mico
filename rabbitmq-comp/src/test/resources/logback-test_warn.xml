<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <!-- encoders are assigned the type
         ch.qos.logback.classic.encoder.PatternLayoutEncoder by default -->
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread{20}] %-5level %logger{60} - %msg%n</pattern>
    </encoder>
  </appender>

  <logger name="eu.mico.platform.camel.MicoRabbitProducer" level="ERROR" />

  <root level="WARN">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>